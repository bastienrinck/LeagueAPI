(sequelize) => {
    sequelize.Summoner = sequelize.define('summoner', {
        id: {type: Sequelize.INTEGER, primaryKey: true},
        accountId: {type: Sequelize.INTEGER},
        name: {type: Sequelize.STRING}
    });

    sequelize.Summoner.sync().then(
        () => console.log("Table summoner created"),
        (err) => console.error(`Error ${err} while creating table 'summoner'`)
    );
};
